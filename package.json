{
  "name": "blog-anasyakubu",
  "description": "A Blog build with Next.js + Sanity",
  "keywords": [
    "Sanity",
    "Next.js",
    "Headless CMS",
    "Visual Editing",
    "Live Content API (LCAPI)",
    "Real-time"
  ],
  "homepage": "https://blog.anasyakubu.com",
  "bugs": {
    "url": "https://github.com/anasyakubu/blog-anasyakubu"
  },
  "workspaces": [
    "studio",
    "frontend"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel --print-label dev:*",
    "dev:next": "npm run dev --workspace=frontend",
    "dev:studio": "npm run dev --workspace=studio",
    "format": "prettier --cache --write .",
    "import-sample-data": "cd studio && sanity dataset import sample-data.tar.gz --replace",
    "lint": "npm run lint --workspace=frontend",
    "type-check": "npm run type-check --workspaces"
  },
  "prettier": "@sanity/prettier-config",
  "devDependencies": {
    "@sanity/prettier-config": "^2.0.1",
    "npm-run-all2": "^5.0.2",
    "prettier": "^3.6.2"
  }
}
